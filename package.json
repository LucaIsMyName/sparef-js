{
  "name": "sparef-js",
  "version": "1.1.1",
  "description": "A TypeScript library to turn a multi-page app into a single-page app with smooth transitions",
  "main": "dist/sparef.js",
  "module": "dist/sparef.mjs",
  "types": "dist/sparef.d.ts",
  "unpkg": "dist/sparef.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build src/sparef.ts",
    "build:umd": "rollup -c rollup.config.js",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "spa",
    "single-page-app",
    "transitions",
    "typescript",
    "view-transitions"
  ],
  "author": "LucaIsMyName",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucaIsMyName/sparef-js.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "rollup": "^2.75.6",
    "typescript": "^4.7.3",
    "@types/jest": "^27.0.3",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^28.0.0",
    "parcel": "^2.8.3",
    "ts-jest": "^28.0.0"
  }
}
